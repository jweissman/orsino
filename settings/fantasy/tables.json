  "roomNarrative": {
    "discriminator": "type",
    "groups": {
      "alcove": [ "You enter a hidden alcove, tucked away behind a crumbling wall." ],
      "altar": [ "You step into a sacred altar, the air filled with the scent of incense. The walls are adorned with ancient symbols, and a powerful aura emanates from the center." ],
      "archive": [ "You enter a dusty archive, the shelves filled with ancient scrolls and tomes. The air is thick with the scent of old paper and ink, and you can hear the faint rustling of pages." ],
      "armory": [ "You step into an armory, the walls lined with racks of weapons and armor. The air is filled with the scent of oil and metal, and you can hear the faint clanging of blacksmiths at work." ],
      "barracks": [ "You enter a barracks, the air filled with the scent of sweat and leather. The walls are lined with bunks and lockers, and you can hear the faint snoring of soldiers." ],
      "cell": [ "You find yourself in a cold, damp cell. The walls are covered in scratches and graffiti, and the air is thick with despair." ],
      "chamber": [ "You step into a dimly lit chamber, the walls adorned with ancient tapestries. A faint glow emanates from a mysterious artifact in the center." ],
      "corridor": [ "You find yourself in a narrow corridor, the stone walls echoing your footsteps. The air is damp and musty, and you can hear distant sounds of movement." ],
      "guardroom": [ "You enter a guardroom, the air filled with the scent of sweat and metal. The walls are lined with weapons and armor, and you can hear the faint chatter of guards." ],
      "hall": [ "You enter a grand hall, the ceiling towering above you. The walls are lined with suits of armor and ancient weapons, and a sense of history hangs in the air." ],
      "lair": [ "You descend into a dark lair, the air thick with the scent of decay. The walls are covered in strange markings, and you can hear the faint sound of breathing." ],
      "library": [ "You enter a dusty library, the shelves filled with ancient tomes and scrolls. The air is thick with the scent of old paper and ink, and you can hear the faint rustling of pages." ],
      "reliquary": [ "You step into a sacred reliquary, the air filled with reverence. The walls are adorned with ancient relics and artifacts, and a powerful aura emanates from the center." ],
      "sanctum": [ "You enter a hidden sanctum, the air thick with mystery. The walls are adorned with ancient symbols, and a powerful aura emanates from the center." ],
      "scriptorium": [ "You step into a quiet scriptorium, the air filled with the scent of parchment and ink. The walls are lined with desks and quills, and you can hear the faint scratching of pens." ],
      "shrine": [ "You enter a sacred shrine, the air filled with reverence. The walls are adorned with ancient symbols and offerings, and a powerful aura emanates from the center." ],
      "tavern": [ "You enter a bustling tavern filled with the scent of ale and roasted meat. The air is thick with laughter and the clinking of mugs." ],
      "throne_room": [ "You enter a grand throne room, the walls lined with towering pillars and the floor covered in intricate mosaics. A majestic throne sits at the far end, guarded by a fearsome creature." ],
      "treasury": [ "You step into a glittering treasury, the walls lined with shelves of gold and jewels. The air is thick with the scent of wealth and power, and you can hear the faint clinking of coins." ],
      "vault": [ "You step into a secure vault, the air heavy with the scent of old wood and metal. The walls are lined with locked chests and safes, and you can hear the faint ticking of hidden mechanisms." ],
      "watchtower": [ "You climb into a watchtower, the air crisp and cold. The walls are lined with windows offering a panoramic view of the surrounding landscape, and you can hear the faint sound of birds." ]
    }
